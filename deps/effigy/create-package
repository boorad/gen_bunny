#! /bin/sh

# DO NOT EDIT THIS FILE.  It is autogenerated by bootstrap.

if test -d bin
  then
    PATH="`pwd`/bin:$PATH"
  fi

if test -d fw/bin
  then
    PATH="`pwd`/fw/bin:$PATH"
    export PATH
  fi

fwp=`which fw-package`

if test -z "$fwp"
  then
    echo "create-package: fatal: fw-package not installed or not in PATH" 1>&2
    exit 1
  fi

"$fwp" --fw_version 0.1.31 --revision none --name effigy --template erlang --revision none "$@"
